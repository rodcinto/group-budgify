{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/group-budgify/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/group-budgify/tsconfig.app.json"
  },
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "rmq": {
      "type": "library",
      "root": "libs/rmq",
      "entryFile": "index",
      "sourceRoot": "libs/rmq/src",
      "compilerOptions": {
        "tsConfigPath": "libs/rmq/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "group-budgify": {
      "type": "application",
      "root": "apps/group-budgify",
      "entryFile": "main",
      "sourceRoot": "apps/group-budgify/src",
      "compilerOptions": {
        "tsConfigPath": "apps/group-budgify/tsconfig.app.json"
      }
    },
    "transactions-app": {
      "type": "application",
      "root": "apps/transactions-app",
      "entryFile": "main",
      "sourceRoot": "apps/transactions-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/transactions-app/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/group-budgify"
}